<?php

/**
 * @file
 *  Install, update and uninstall functions for calculoid module.
 */

/**
 * Implements hook_uninstall().
 */
function calculoid_uninstall() {
  variable_del('calculoid_api_key');
  variable_del('calculoid_app_name');
  variable_del('calculoid_app_controller');
  variable_del('calculoid_show_title');
  variable_del('calculoid_show_description');
}
